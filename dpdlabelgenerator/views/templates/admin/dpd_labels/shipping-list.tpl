<div id="shippin_list_container">	<div id="dpd_sender_address">		<h2>{l s='Client Address'}</h2>		<p>{$sender.company}<br>{$sender.address1}<br>{$sender.iso_code}-{$sender.postcode} {$sender.city}</p>	</div>	<div id="dpd_logo"><h2><img src="{$logo_path}" alt="DPD Logo" /></h2></div>		<div id="shipping_list">		<h2 class="visible-print">{l s='Shipping List'} - {$shipments|@count} {l s='parcel(s)'}</h2>		<table class="table" id="shipping_list_table">			<thead>				<tr>					<th>						<span class="title_box ">{l s='Count'}</span>					</th>					<th>						<span class="title_box ">{l s='Track Number'}</span>					</th>					<th>						<span class="title_box ">{l s='Delivery Type'}</span>					</th>					<th>						<span class="title_box ">{l s='First Name'}</span>					</th>					<th>						<span class="title_box ">{l s='Last Name'}</span>					</th>					<th>						<span class="title_box ">{l s='Address'}</span>					</th>					<th>						<span class="title_box ">{l s='Country'}</span>					</th>					<th>						<span class="title_box ">{l s='Zip Code'}</span>					</th>					<th>						<span class="title_box ">{l s='City'}</span>					</th>					<th>						<span class="title_box ">{l s='Ref 1'}</span>					</th>					<th>						<span class="title_box ">{l s='Weight'}</span>					</th>				</tr>			</thead>			<tbody>				{foreach $shipments as $key => $shipment}					<tr id="shipment_{$shipment.tracking_number}">						<td>{$key+1}</td>						<td>{$shipment.tracking_number}</td>						<td>{$shipment.name}</td>						<td>{$shipment.firstname}</td>						<td>{$shipment.lastname}</td>						<td>{$shipment.address1}</td>						<td>{$shipment.iso_code}</td>						<td>{$shipment.postcode}</td>						<td>{$shipment.city}</td>						<td>{$shipment.reference}</td>						<td>{$shipment.weight}</td>					</tr>				{/foreach}			</tbody>		</table>	</div>	<div id="dpd_signature">		<h2>{l s='Signature'}</h2>		<div id="dpd_signature_box" />	</div></div>